<h1>ユーザー一覧</h1>

<table>
  <thead>
  <tr>
    <th><%= User.human_attribute_name(:email) %></th>
    <th colspan="1"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= link_to '詳細', user %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
<%= paginate @users %>